    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <link rel="stylesheet" href="/style.css">
    <!-- font-awosome -->
    <script src="https://kit.fontawesome.com/9967b4b30f.js" crossorigin="anonymous"></script>
</head>

<body>
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
        crossorigin="anonymous"></script>
    <!-- Nav-bar -->
    <section id="nav-bar">
        <nav class="navbar navbar-expand-lg">
            <div class="container-fluid">
                <img class = "nav-brand-img"src="data:image/png;base64,"
                    alt="Logo" width="30" height="32" class="d-inline-block align-text-top">
                <a class="navbar-brand" href="/">Calender</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <ul class="navbar-nav mb-lg-0 ms-auto">
                        <li class="nav-item me-4">
                            <a class="months-display nav-i nav-link"></a>
                        </li>
                        <li class="nav-item dropdown me-4">
                            <a class="nav-i nav-link dropdown-toggle" href="/" role="button" data-bs-toggle="dropdown"
                                aria-expanded="false">
                                <%= Period %>
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="Day">
                                        <p class="nav-drp-1">Day</p>
                                        <p class="nav-drp-2">D</p>
                                    </a></li>
                                <li><a class="dropdown-item" href="Week">
                                        <p class="nav-drp-1">Week</p>
                                        <p class="nav-drp-2">W</p>
                                    </a></li>
                                <li><a class="dropdown-item" href="Month">
                                        <p class="nav-drp-1">Month</p>
                                        <p class="nav-drp-2">M</p>
                                    </a></li>
                                <li><a class="dropdown-item" href="Year">
                                        <p class="nav-drp-1">Year</p>
                                        <p class="nav-drp-2">Y</p>
                                    </a></li>
                                <li>
                                    <hr class="dropdown-divider">
                                </li>
                                <li><a class="dropdown-item" href="#"><i class="fa fa-solid fa-check"></i> Show
                                        Weakends</a></li>
                            </ul>
                        </li>

                        <li class="nav-i nav-item me-1">
                            <a class="less-than nav-i nav-link"><i class="fa-regular fa-less-than"></i></a>
                        </li>
                        <li class="nav-i nav-item me-4">
                            <a class="greater-than nav-i nav-link"><i class="fa-regular fa-greater-than"></i></a>
                        </li>
                    </ul>
                        <button class="btn btn-outline-success nav-today" type="submit">Today</button>
                    </form>
                </div>
            </div>
        </nav>
    </section>
    <form action="/Day" name = "myForm" method="post">
        <input class = "hidden" type="hidden" name = "date">
    </form>
    <script>
        
        document.querySelector("html").addEventListener("keypress", function (e) {
            var time = new Date;
            if (e.key === "M" || e.key === "m"){
                window.location.href = "/Month";
            }
            else if (e.key === "W" || e.key === "w"){
                window.location.href = "/";
            }
            else if (e.key === "Y" || e.key === "y"){
                window.location.href = "/Year";
            }
            else if (e.key === "D" || e.key === "d"){
                let str = time.getDate() + "," + time.getDay() + "," + time.getMonth() + "," + time.getFullYear();
                document.querySelector(".hidden").setAttribute("value", str);
                document.myForm.submit();
            }
        })
    </script>
    <% if(Period === "Week"){ %>
        <%- include('weekName') %>
        <%- include('holiday') %>
        <%- include('footer') %>
    <% } %>
    <% if(Period === "Day"){ %>
        <%- include('dayName') %>
        <%- include('dayEvent') %>
        <%- include('footer') %>
    <% } %>
    <% if(Period === "Month"){ %>
        <%- include('MonthEvent') %>
        <%- include('footer') %>
    <% } %>
    <% if(Period === "Year"){ %>
        <%- include('yearEvent') %>
        <%- include('footer') %>
 <% } %>